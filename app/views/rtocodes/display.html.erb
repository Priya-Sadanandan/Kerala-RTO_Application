<p id="notice"><%= notice %></p>


<% if @rtocodes.blank? %>
  <h1>There are no entries containing the term <%= params[:search] %>.</h1>
  else

<h1>Rtocodes</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Office</th>
      <th>District</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rtocodes.each do |rtocode| %>
      <tr>
        <td><%= rtocode.code %></td>
        <td><%= rtocode.office %></td>
        <td><%= rtocode.district %></td>
        <td><%= link_to 'Show', rtocode %></td>
        <td><%= link_to 'Edit', edit_rtocode_path(rtocode) %></td>
        <td><%= link_to 'Destroy', rtocode, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% end %>

<br>
<br>
<%= link_to 'Search again', search_rtocode_path %>
~                                                                                                                                                                                                           
~                                                         
